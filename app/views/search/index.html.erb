<h1>Results containing "<%= params[:q] %>":</h1>

<% @tags.each do |tag| %>
  <h3><%= tag.title %></h3>
  <h5><%= tag.category %></h5>
  <h5><%= tag.comment %></h5>
  <h5><%= tag.address %></h5>
  <h5><%= tag.latitude %></h5>
  <h5><%= tag.latitude %></h5>
<% end %>

<%= debug @tags %>

<div>
<%= image_tag "http://maps.google.com/maps/api/staticmap?size=450x300&sensor=false&zoom=16&markers=#{@tag.latitude}%2C#{@tag.longitude}" %>
</div>